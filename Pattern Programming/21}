void getStarPattern(int n) {
    // Write your code here.
    int i , j;
    for (i =1 ;i<=n ; i++)
    {
        for(j =1 ;j <=n ; j++)
        {
            if (i==1 || j == 1)
            {
                cout <<"*";
            }
            else if (i == n || j==n)
            {
                cout <<"*";
            }
            else{

                cout << " ";
            }
        }
        cout << "\n";
    }
}
